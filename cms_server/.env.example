# 管理员信息
ADMIN_USER=your_admin_username
ADMIN_EMAIL=your_admin_email@example.com
ADMIN_PWD=your_admin_password

# 数据库配置
DB_URI=mongodb://localhost:27017/cms_dev

# Redis 配置
REDIS_URI=redis://localhost:6379
REDIS_PWD=your_redis_password

# 应用端口
PORT=25551

# JWT 配置
JWT_SECRET=your_jwt_secret
JWT_REFRESH_SECRET=your_jwt_refresh_secret
JWT_EXPIRES_IN=1d
JWT_REFRESH_EXPIRES_IN=7d

# 头像 URL 加密
AVATAR_URL_SECRET=your_avatar_url_secret

# 邮件通知配置
NOTIFY_EMAIL_USER=your_email@example.com
NOTIFY_EMAIL_PWD=your_email_app_password

# 加密密钥（32位字符）
ENCRYPTION_KEY=your_32_char_encryption_key

# MinIO 配置
MINIO_END_POINT=localhost
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=your_minio_access_key
MINIO_SECRET_KEY=your_minio_secret_key
MINIO_PUBLIC_HOST=localhost
MINIO_BUCKET=assets
